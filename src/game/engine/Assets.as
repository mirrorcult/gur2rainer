      public static function playWin() : void
      {
         playVoice(Assets["VcPlayerWin" + (Math.floor(Math.random() * 20) + 1)]);
      }
      
      public static function playGrappleBoss() : void
      {
         if(FP.choose(true,false,false))
         {
            return;
         }
         playVoice(Assets["VcGrappleBoss" + (Math.floor(Math.random() * 16) + 1)]);
      }
      
      public static function get musicPlaying() : Boolean
      {
         return music != null;
      }
      
      public static function playVoice(vc:Sfx) : void
      {
         if(!Options.voices)
         {
            return;
         }
         if(voice.playing)
         {
            voice.stop();
         }
         voice = vc;
         vc.play(VOICE_VOL);
      }
      
      public static function setMusic(to:Sfx = null, loop:Boolean = true) : void
      {
         if(music == to || FP.volume == 0)
         {
            return;
         }
         if(music)
         {
            music.stop();
         }
         music = to;
         if(music != null)
         {
            if(loop)
            {
               music.loop();
            }
            else
            {
               music.complete = setMusic;
               music.play();
            }
         }
      }
      
      public static function playDeath() : void
      {
